#include <stdio.h>
#include <stdlib.h>
int main(void) {
	setbuf(stdout,NULL);
	int numero1;
	int numero2;
	int resultado;

	printf("Ingrese el primer numero");
	scanf("%d",&numero1);
	printf("Ingrese el segundo numero");
	scanf("%d",&numero2);
	resultado= numero1+numero2;
	printf("la suma del total es de  : %d", resultado);

	return EXIT_SUCCESS;
}
