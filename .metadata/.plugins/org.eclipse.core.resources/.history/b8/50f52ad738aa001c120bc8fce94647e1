#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int numeros=7;
	float numerosdecimales=7.5;

	printf("aca voy a mostrar el numero : %d/n ", numeros);
	printf("aca voy a mostrar el numero con decimal :%f " , numerosdecimales);
}
