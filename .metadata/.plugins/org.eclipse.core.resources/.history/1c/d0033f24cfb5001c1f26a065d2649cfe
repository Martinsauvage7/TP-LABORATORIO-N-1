#include <stdio.h>
#include <stdlib.h>

float Dividir(float dividendo,float divisor);

int main(void)
{
	setbuf(stdout, NULL);
	float resultado;

	resultado= Dividir(10,5);
}

float Dividir(float dividendo,float divisor)
	{
		float rtn;
		if(divisor ==0)
			{
			printf("No se puede dividir por 0\n");
			}
			else
			{
				rtn= dividendo/divisor;
				printf("La division es %f\n" , rtn);
			}
		return rtn;
	}






