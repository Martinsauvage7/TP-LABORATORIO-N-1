float Dividir(float dividendo,float divisor);

int main(void)
{
	setbuf(stdout, NULL);
	float resultado;
	float divisor=0;

	resultado= Dividir(10,5)
	printf("la division es %f\n", resultado);
	return 0;
}
		float Dividir(float dividendo,float divisor);
		if(divisor !=0)
					{
					resultado= Dividir(20,5);
					printf("la division es %f\n", resultado);
					}
				else
				{
					printf("el numero ingresado no es posible dividirlo\n");
